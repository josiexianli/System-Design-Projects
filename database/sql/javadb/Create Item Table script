//create item table
CREATE TABLE `item`.`item` (
  `item_id` INT NOT NULL,
  `isReserved` TINYINT NOT NULL,
  `reserve_user_id` INT NULL,
  'email' varchar(50) null,
  PRIMARY KEY (`item_id`));

//create user table
CREATE TABLE `user`.`user` (
  `username` varchar(16) NOT NULL,
  `password` varchar(32) NOT NULL,
  `user_id` int NOT NULL,
  `usertype` INT NULL,
  PRIMARY KEY (`user_id`));

//load user table with data
INSERT INTO user values ('test1', 'password1', 0, 1);
INSERT INTO user values ('test2', 'password2', 1, 2);
INSERT INTO user values ('test3', 'password3', 2, 3);

//load user table with data
INSERT INTO item values (1, true, 1);
